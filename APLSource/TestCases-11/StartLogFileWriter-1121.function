 handle←StartLogFileWriter filename;⎕WX;cmd;rc
 ⎕WX←3
 cmd←⊂'"',(↑#.GetCommandLineArgs),'"'
 cmd,←⊂'"',filename,'"'
 cmd,←⊂'"',#.FilesAndDirs.PWD,'\Tests\WriteLogFile.dws"'
 :If 0
     cmd,←⊂'-lx'
 :EndIf
 rc←#.OS.WinExecute∊cmd,¨' '
 :Repeat
     handle←#.WinSys.GetHandleFromCaption'LogDog_Test_LogFileWriter'
 :Until 0≠handle
⍝Done
